
package ch.hearc.cours.moo.fibo;

public class Fibo implements Runnable
	{
	
	/*------------------------------------------------------------------*\
	|*							Constructeurs							*|
	\*------------------------------------------------------------------*/
	
	public Fibo(int n)
		{
		this.n = n;
		this.tabSuite = new BigInteger[n];
		}
		
	/*------------------------------------------------------------------*\
	|*							Methodes Public							*|
	\*------------------------------------------------------------------*/
	
	@Override
	public void run()
		{
		// TODO Auto-generated method stub
		
		}
		
	/*------------------------------*\
	|*				Get				*|
	\*------------------------------*/
	
	public Integer[] getTabSuite()
		{
		return this.tabSuite;
		}
		
	/*------------------------------------------------------------------*\
	|*							Methodes Private						*|
	\*------------------------------------------------------------------*/
	
	/*------------------------------------------------------------------*\
	|*							Attributs Private						*|
	\*------------------------------------------------------------------*/
	
	// Inputs
	private int n;
	
	// Outputs
	private Integer[] tabSuite;
	
	}
