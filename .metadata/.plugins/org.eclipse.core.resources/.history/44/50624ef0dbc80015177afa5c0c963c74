
package ch.hearc.cours.moo.fibo;

public class UseFibo
	{

	/*------------------------------------------------------------------*\
	|*							Methodes Public							*|
	\*------------------------------------------------------------------*/

	public static void main(String[] args)
		{
		main();
		}

	public static void main()
		{
		int n = 500;
		Fibo fibo = new Fibo(n);
		System.out.println(fibo);
		fibo.run();
		System.out.println(fibo);
		}

	/*------------------------------------------------------------------*\
	|*							Methodes Private						*|
	\*------------------------------------------------------------------*/

	}
