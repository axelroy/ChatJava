
package ch.hearc.cours.moo.heritage.formes;

public class Carre extends Rectangle
	{
	
	/*------------------------------------------------------------------*\
	|*							Constructeurs							*|
	\*------------------------------------------------------------------*/
	
	public Carre(String nom, double c)
		{
		super(nom, c, c);
		}
		
	public Carre(Carre source)
		{
		super(source);
		}
		
	/*------------------------------------------------------------------*\
	|*							Methodes Public							*|
	\*------------------------------------------------------------------*/
	
	@Override
	public Formes cloneOf()
		{
		return new Carre(this);
		}
		
	/*------------------------------------------------------------------*\
	|*							Methodes Private						*|
	\*------------------------------------------------------------------*/
	
	/*------------------------------------------------------------------*\
	|*							Attributs Private						*|
	\*------------------------------------------------------------------*/
	}
