
package ch.hearc.cours.moo.heritage.formes;

public class Cercle extends Ellipse
	{
	
	/*------------------------------------------------------------------*\
	|*							Constructeurs							*|
	\*------------------------------------------------------------------*/
	
	public Cercle(String nom, double r)
		{
		super(nom, r, r);
		}
		
	public Cercle(Cercle source)
		{
		super(source);
		}
	
	/*------------------------------------------------------------------*\
	|*							Methodes Public							*|
	\*------------------------------------------------------------------*/
	
	@Override
	public Cercle cloneOf()
		{
		return new Cercle(this);
		}
	
	/*------------------------------------------------------------------*\
	|*							Methodes Private						*|
	\*------------------------------------------------------------------*/
	
	/*------------------------------------------------------------------*\
	|*							Attributs Private						*|
	\*------------------------------------------------------------------*/
	
	}
