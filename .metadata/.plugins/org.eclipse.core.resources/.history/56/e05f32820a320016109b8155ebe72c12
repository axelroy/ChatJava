
package ch.hearc.cours.moo.interfaces;

public class Homme implements Vivre_I
	{

	/*------------------------------------------------------------------*\
	|*							Constructeurs							*|
	\*------------------------------------------------------------------*/

	/*------------------------------------------------------------------*\
	|*							Methodes Public							*|
	\*------------------------------------------------------------------*/

	@Override
	public void courir()
		{
		System.out.println("Courir");
		}

	@Override
	public void marcher()
		{
		System.out.println("Marcher");
		}

	@Override
	public void manger()
		{
		System.out.println("Manger");
		}

	@Override
	public void respirer()
		{
		System.out.println("Respirer");
		}

	/*------------------------------*\
	|*				Set				*|
	\*------------------------------*/

	/*------------------------------*\
	|*				Get				*|
	\*------------------------------*/

	/*------------------------------------------------------------------*\
	|*							Methodes Private						*|
	\*------------------------------------------------------------------*/

	/*------------------------------------------------------------------*\
	|*							Attributs Private						*|
	\*------------------------------------------------------------------*/
	}
